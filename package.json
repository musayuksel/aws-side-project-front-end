    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx}\""
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.760.0",
    "amazon-cognito-identity-js": "^6.3.12",
